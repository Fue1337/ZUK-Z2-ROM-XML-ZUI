<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.0dip" />
            <stroke android:width="1.0px" android:color="#007d7d7d" />
            <solid android:color="#ff7d7d7d" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <stroke android:width="1.0px" android:color="#001499f7" />
                <solid android:color="#ff1499f7" />
            </shape>
        </clip>
    </item>
</layer-list>